				<string>#!/bin/bash

cat &lt;&lt; EOB
&lt;?xml version="1.0"?&gt;
&lt;items&gt;
EOB

SYMBOLS=$(cat "$(pwd)/symbols" | sort -u | grep -v '^$');


for sym in $SYMBOLS; do
    RESULT=$(curl -s "http://download.finance.yahoo.com/d/quotes.csv?s=$sym&amp;f=sl1c1&amp;e=.csv"|sed -e 's/,/  /g'|sed -e 's/"//g')

SYMBL=$(echo $RESULT|cut -d ' ' -f1);
PRICE=$(echo $RESULT|cut -d ' ' -f2)
CHNG=$(echo $RESULT|cut -d ' ' -f3)

$(echo $CHNG|grep -q '^-')
ICON_SFX=$?

cat &lt;&lt; EOB
  &lt;item uid="$SYMBL" valid="No"&gt;
    &lt;title&gt;$SYMBL $PRICE&lt;/title&gt;
    &lt;subtitle&gt;$CHNG&lt;/subtitle&gt;
    &lt;icon&gt;stock-$ICON_SFX.png&lt;/icon&gt;
  &lt;/item&gt;
EOB
done

cat &lt;&lt; EOB
&lt;/items&gt;
EOB
